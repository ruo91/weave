This is the shape of unencrypted `setup-ci-secrets`

cat > /tmp/boto_gsutil_cert.pem << EOF
-----BEGIN PRIVATE KEY-----
<...>
-----END PRIVATE KEY-----
EOF

cat > ~/.boto << EOF
[Credentials]
gs_service_client_id = <...>
gs_service_key_file = /tmp/boto_gsutil_cert.pem

[Boto]
https_validate_certificates = True

[GSUtil]
content_language = en
default_api_version = 2
default_project_id = <...>
EOF
